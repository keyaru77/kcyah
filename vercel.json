{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "node_modules/chrome-aws-lambda/**": {
      "includeFiles": ["**"]
    },
    "node_modules/puppeteer-core/**": {
      "includeFiles": ["**"]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
